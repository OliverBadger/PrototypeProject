@page
@model PrototypeProject.Pages.Manager.EditModel
@{
}
<form method="post">

    <input type="hidden" asp-for="Stock.Id" />


    <div class="form-group">
        <label asp-for="Stock.Title" class="control-label"></label>
        <input type="text" asp-for="Stock.Title" class="form-control" />
    </div>

    <div class="form-group">
        <label asp-for="Stock.Count" class="control-label"></label>
        <input type="text" asp-for="Stock.Count" class="form-control" />
    </div>

    <div class="form-group">
        <label asp-for="Stock.Price" class="control-label"></label>
        <input type="text" asp-for="Stock.Price" class="form-control" />
    </div>

    <div class="form-group">
        <input type="submit" value="Update" class="btn btn-primary" />
    </div>

</form>